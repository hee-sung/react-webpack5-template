{
  "compilerOptions": {
    "baseUrl": "/src",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ES6",
    "target": "ES5",
    "jsx": "react",
//    "lib": [
//      "dom",
//      "dom.iterable",
//      "esnext"
//    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "src"
  ]
}